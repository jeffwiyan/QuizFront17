// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Inspiration {
  id          String   @id @default(cuid())
  title       String
  category    String
  description String
  createdAt   DateTime @default(now())
}
